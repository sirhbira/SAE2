<BorderPane prefHeight="500.0" prefWidth="500.0" styleClass="background" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sae.controleur.Controleur">
   <bottom>
      <HBox focusTraversable="true" prefHeight="124.0" prefWidth="400.0" BorderPane.alignment="CENTER">
